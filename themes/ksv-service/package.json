{
  "private": true,
  "name": "ksv-service",
  "version": "1.0.0",
  "description": "The wizard installation is the recommended way to install October for **non-technical users**. It is simpler than the command-line installation and doesn't require any special skills.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production",
    "partytown": "partytown copylib public/~partytown"
  },
  "browserslist": [
    "defaults",
    "ie 11"
  ],
